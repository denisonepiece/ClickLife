    form(name="pass")
      +input-password()
      +input-password('Подтвердите пароль')

      +button('big-fill-log', 'Войти в кабинет')

    span.btn-close
      include ../btn-close.svg
    span.modal-role Регистрация
      include ../../../../assets/img/icon-enter.svg